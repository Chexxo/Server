{
  "name": "chexxo-server",
  "version": "0.0.1",
  "description": "The server component for the chexxo certificate quality identification plugin.",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --write ./src/",
    "lint:eslint": "npx eslint ./src/ --max-warnings 0",
    "test": "npx jest --coverage true",
    "test:watch": "npx jest --watch",
    "build:prod": "./node_modules/.bin/tsc -p ./tsconfig.prod.json",
    "build:express": "npm run build:prod && node ./express-package.js",
    "build:documentation": "npx typedoc --options ./typedoc.config.js",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chexxo/Server.git"
  },
  "author": "Carlo Kirchmeier, Yannick Vogt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chexxo/Server/issues"
  },
  "homepage": "https://github.com/Chexxo/Server",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.10",
    "@types/jsrsasign": "^8.0.8",
    "@types/node": "^14.14.0",
    "@types/node-forge": "^0.9.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.6",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typedoc": "^0.18.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-no-inherit": "^1.2.0",
    "typescript": "^4.0.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jsrsasign": "^10.0.4"
  }
}
